{
  "name": "@tresjs/cientos",
  "type": "module",
  "version": "4.0.3",
  "packageManager": "pnpm@9.15.0",
  "description": "Collection of useful helpers and fully functional, ready-made abstractions for Tres",
  "author": "Alvaro Saburido <hola@alvarosaburido.dev> (https://github.com/alvarosabu/)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tresjs/cientos.git"
  },
  "keywords": [
    "vue",
    "3d",
    "threejs",
    "three",
    "threejs-vue",
    "composables"
  ],
  "maintainers": [
    "Alvaro Saburido (https://github.com/alvarosabu/)",
    "Andre Tchen (https://github.com/andretchen0)"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/trescientos.js"
    },
    "./core": {
      "types": "./dist/core/index.d.ts"
    },
    "./*": "./*"
  },
  "main": "./dist/trescientos.js",
  "module": "./dist/trescientos.js",
  "types": "./dist/index.d.ts",
  "files": [
    "*.d.ts",
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite serve playground/vue",
    "dev:nuxt": "cd playground/nuxt && pnpm dev",
    "build": "vite build",
    "release": "release-it",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "update": "tsx scripts/update.ts",
    "docs:dev": "tsx scripts/update.ts && vitepress dev src",
    "docs:build": "tsx scripts/update.ts && vitepress build src",
    "docs:preview": "tsx scripts/update.ts && vitepress preview src"
  },
  "peerDependencies": {
    "@tresjs/core": ">=4.2.1",
    "three": ">=0.133",
    "unplugin-auto-import": ">=0.19.0",
    "vue": ">=3.3"
  },
  "dependencies": {
    "@vueuse/core": "^12.0.0",
    "camera-controls": "^2.9.0",
    "stats-gl": "^2.4.2",
    "stats.js": "^0.17.0",
    "three-custom-shader-material": "^5.4.0",
    "three-stdlib": "^2.35.2"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^9.0.3",
    "@tresjs/core": "^4.3.1",
    "@tresjs/eslint-config": "^1.4.0",
    "@tresjs/leches": "^0.14.0",
    "@types/node": "^22.10.2",
    "@types/three": "^0.170.0",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "@vitejs/plugin-vue": "^5.2.1",
    "eslint": "^9.17.0",
    "eslint-plugin-vue": "^9.32.0",
    "esno": "^4.8.0",
    "gray-matter": "^4.0.3",
    "gsap": "^3.12.5",
    "kolorist": "^1.8.0",
    "markdown-it": "^14.0.1",
    "pathe": "^1.1.2",
    "release-it": "^17.10.0",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "three": "^0.171.0",
    "typescript": "^5.7.2",
    "unocss": "^0.65.2",
    "unplugin-vue-components": "^0.27.5",
    "vite": "^6.0.5",
    "vite-plugin-dts": "4.3.0",
    "vite-plugin-glsl": "^1.3.1",
    "vite-plugin-qrcode": "^0.2.3",
    "vite-svg-loader": "^5.1.0",
    "vitepress": "1.5.0",
    "vue-router": "^4.5.0"
  },
  "resolutions": {
    "@tresjs/cientos": "workspace:*"
  }
}
